$(".kt-subheader__breadcrumbs").each(function () { $(this).replaceWith('<div class="kt-subheader__breadcrumbs">' + $(this).html() + "</div>") }), $(".breadcrumbs_item").each(function () { $(this).attr("class").indexOf("active") >= 0 ? $(this).replaceWith('<a href="javascript:0;" class="kt-subheader__breadcrumbs-link">' + $(this).text() + "</a>") : $(this).replaceWith($(this).children().addClass("kt-subheader__breadcrumbs-link")) }), $(".kt-subheader__breadcrumbs-link").each(function () { $(this).html($(this).text()) });